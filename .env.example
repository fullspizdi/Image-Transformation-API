# .env.example

# This is an example of the environment variables needed for the Image Transformation API.
# Copy this file to '.env' in the root directory and update the values accordingly.

# Server port (default is 3000 if not specified)
PORT=3000

# API key for authenticating requests
API_KEY=your_api_key_here

# Redis URL for caching and rate limiting
REDIS_URL=redis://localhost:6379

# Path to store uploaded images
IMAGE_STORAGE_PATH=uploads/
